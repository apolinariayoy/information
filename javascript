function f(){
    let a = document.getElementById("p").value = "(2x<sup>2</sup>+ 4)<sup>2</sup> + (2x<sup>2</sup>+ 4)<sup>2</sup> - 2(2x<sup>2</sup> + 4)(2x<sup>2</sup> - 4) = 4x<sup>4</sup> + 16x<sup>2</sup> + 16 + 4x<sup>4</sup> - 16x<sup>2</sup> + 16 - 2(4x<sup>4</sup> - 16) = 8x<sup>4</sup> + 32 - 8x<sup>4</sup> + 32 = 64";
